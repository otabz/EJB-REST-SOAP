        <xs:restriction base="xs:string">
            <xs:enumeration value="INPATIENT" />
            <xs:enumeration value="OUTPATIENT" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="caseTypeST">
        <xs:annotation>
            <xs:documentation>
                The type of case or approval the request includes
                
                Possible values are:
                INPATIENT, OUTPATIENT, DENTAL, DELIVERY, OPTICAL, PHARMACY
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="INPATIENT" />
            <xs:enumeration value="OUTPATIENT" />
